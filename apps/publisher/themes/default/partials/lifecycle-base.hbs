<div class="row">

    <div id='lifecycle-global-notifications' class="col-xs-12 col-sm-12 col-md-12 col-lg-12"></div>
    <div class="col-xs-12 col-sm-12 col-md-12 col-lg-12">
        <div id='lifecycle-asset-details'>
            {{#with assets}}
                <div class="well asset-well">
                    <div class="container-fluid">
                        <div class="row">
                            <div class="col-lg-1">
                                <div class="ast-img setbgcolor" style="background: {{this.uniqueColor}}">
                        <span class="ast-type-icon">
                            <i class="{{icon}} fw-lg"></i>
                        </span>

                                    <div class="ast-name-icon">{{this.nameToChar}}</div>
                                </div>
                            </div>
                            <div class="col-lg-10 padding-left-double">
                                <h4>{{name}}</h4>
                                {{#if version}}
                                    <p>Version : {{version}}</p>
                                {{/if}}
                                {{#if lifecycleState}}
                                    <p>{{lifecycle}} : {{lifecycleState}}</p>
                                {{/if}}
                                {{#if createdDate}}
                                <div class="well-description">{{createdDate}}</div>
                                {{/if}}
                            </div>
                        </div>
                    </div>
                </div>
            {{/with}}
        </div>
        {{> lifecycle-selection-area}}
    </div>
    <div id='lifecycle-information' class="lifecycle-information-area col-xs-12 col-sm-12 col-md-12 col-lg-12">
        {{> lifecycle-information}}
    </div>
    {{> lifecycle-rendering-view}}
</div>